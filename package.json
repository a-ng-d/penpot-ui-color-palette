{
  "name": "penpot-ui-color-palette",
  "version": "0.0.1",
  "private": true,
  "description": "UI Color Palette is a penpot plugin that creates, manages, deploys, and publishes consistent and accessible color palettes.",
  "type": "module",
  "workspaces": [
    "packages/ui-ui-color-palette",
    "packages/ui-ui-color-palette/packages/auth-yelbolt",
    "packages/ui-ui-color-palette/packages/auth-yelbolt-worker",
    "packages/ui-ui-color-palette/packages/announcements-yelbolt-worker"
  ],
  "scripts": {
    "install:packages": "npm install --workspaces",
    "update:packages": "git submodule update --remote --merge",
    "update:figmug": "npm i @a_ng_d/figmug-ui@latest @a_ng_d/figmug-utils@latest",
    "update:uicp": "npm i @a_ng_d/utils-ui-color-palette@latest",
    "start:ext": "npm run start:packages -w ui-ui-color-palette",
    "start:dev": "npm run build & vite preview",
    "build": "npx vite build --mode development & IS_PLUGIN=true npx vite build --mode development",
    "build:prod": "npx vite build --mode production & IS_PLUGIN=true npx vite build --mode production",
    "typecheck": "npx tsc --noEmit",
    "lint": "npx eslint ./src/** --fix --ext .ts,.tsx .",
    "format": "npx prettier './src' --write"
  },
  "keywords": [
    "color",
    "color-scheme",
    "lch",
    "color-system",
    "ui-color",
    "penpot-plugin"
  ],
  "author": "Aur√©lien Grimaud",
  "licence": "MIT",
  "repository": "https://github.com/a-ng-d/penpot-ui-color-palette",
  "devDependencies": {
    "@penpot/plugin-types": "^1.2.0",
    "@preact/preset-vite": "^2.10.1",
    "@sentry/vite-plugin": "^3.3.1",
    "@types/chroma-js": "^2.4.0",
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.14.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.25.3",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.1.2",
    "typescript": "^5.4.3",
    "vite": "^6.2.5",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-sentry": "^1.4.1",
    "vite-plugin-singlefile": "^2.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@a_ng_d/figmug-ui": "^1.11.3",
    "@a_ng_d/figmug-utils": "^0.3.5",
    "@a_ng_d/utils-ui-color-palette": "^1.3.5",
    "@nanostores/preact": "^0.5.2",
    "@sentry/react": "^8.8.0",
    "@supabase/supabase-js": "^2.44.2",
    "@types/apca-w3": "^0.1.3",
    "apca-w3": "^0.1.9",
    "chroma-js": "^2.4.2",
    "file-saver": "^2.0.5",
    "hsluv": "^1.0.1",
    "jszip": "^3.10.1",
    "mixpanel-figma": "^2.0.4",
    "preact": "^10.6.4",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "uid": "^2.0.2"
  }
}
